<%- communities.each do |community| %>
  <%= render 'profile_card', { name: community.name,
                               become_members: { url: new_community_membership_path(community.slug),
                                                 text: 'Become a Member' },
                               view_members:   { url: community_memberships_path(community.slug),
                                                 text: 'View Members' } } %>
<%- end %>